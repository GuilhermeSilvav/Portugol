programa
{
	
	funcao inicio()
	{
		inteiro numero[13] = {10,9,80,72,74,5,62,36,44,7,3,17,66}
		inteiro menor[12], numeroMaior
		
		para(inteiro i = 0; i <=12; i ++){
			
			para(inteiro j = i; j<=12; j++){
				se(numero[i] > numero[j]){
				
